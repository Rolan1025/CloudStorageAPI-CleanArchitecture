{
  "ConnectionStrings": {
    "AzureTableStorage": "DefaultEndpointsProtocol=https;AccountName=<your-account-name>;AccountKey=<your-account-key>;EndpointSuffix=core.windows.net",
    "AzureBlobLogs": "DefaultEndpointsProtocol=https;AccountName=<your-blob-account-name>;AccountKey=<your-blob-account-key>;EndpointSuffix=core.windows.net"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AZURE_KEY_VAULT_ENDPOINT": "https://<your-key-vault-name>.vault.azure.net/",
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "https://<your-issuer-url>",
    "Key": "<your-jwt-secret-key>"
  }
}
